    <SERVICE name="objectseed" loadPolicy="demand">
       <PROP name="impl" value="org.isam.exehda.services.primos.ObjectSeedImpl"/>
    </SERVICE>
    
    <SERVICE name="collector" loadPolicy="boot">
       <PROP name="impl" value="org.isam.exehda.services.primos.CollectorImplNode"/>
